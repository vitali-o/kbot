pipeline {
    agent any
    parameters {
        choice(name: 'OS', choices: ['linux', 'android', 'windows'], description: 'Pick OS')
        choice(name: 'ARCH', choices: ['386', 'arm', 'arm64', 'amd64'], description: 'Pick ARCH')
    }
    stages {
        stage('test') {
            steps {
                script {
                    echo "run test"
                    echo "make test"
                }
            }
        }
    }
}
